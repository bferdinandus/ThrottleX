# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# All files
[*]
indent_style = space
indent_size = 4
insert_final_newline = true
end_of_line = lf

[*.cs]
max_line_length = 250

[{*.xml,*.csproj,*.json,*.props,*.targets}]
indent_size = 2
